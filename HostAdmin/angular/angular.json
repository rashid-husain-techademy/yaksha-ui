{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "Identity": {
      "i18n": {
        "locales": {
          "en": {
            "translation": "src/translate/messages.en.xlf",
            "baseHref": ""
          }
        }
      },
      "schematics": {
        "@schematics/angular:component": {
          "style": "scss"
        }
      },
      "root": "",
      "sourceRoot": "src",
      "projectType": "application",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist",
            "index": "src/index.html",
            "main": "src/main.ts",
            "tsConfig": "src/tsconfig.json",
            "polyfills": "src/polyfills.ts",
            "allowedCommonJsDependencies": [
              "file-saver"
            ],
            "assets": [
              "src/assets",
              "src/favicon.ico",
              "src/web.config",
              {
                "glob": "abp.signalr.js",
                "input": "node_modules/abp-web-resources/Abp/Framework/scripts/libs",
                "output": "/assets/abp"
              },
              {
                "glob": "abp.signalr-client.js",
                "input": "node_modules/abp-web-resources/Abp/Framework/scripts/libs",
                "output": "/assets/abp"
              },
              {
                "glob": "**/*",
                "input": "node_modules/monaco-editor",
                "output": "/assets/monaco-editor"
              }
            ],
            "styles": [
              "node_modules/bootstrap/dist/css/bootstrap.min.css",
              "node_modules/chartist/dist/chartist.min.css",
              "node_modules/flatpickr/dist/flatpickr.css",
              "node_modules/c3/c3.min.css",
              "node_modules/datatables.net-dt/css/jquery.dataTables.css",
              "node_modules/ngx-toastr/toastr.css",
              "node_modules/quill/dist/quill.snow.css",
              "node_modules/quill/dist/quill.bubble.css",
              "node_modules/angular-calendar/css/angular-calendar.css",
              "node_modules/@swimlane/ngx-datatable/index.css",
              "node_modules/@swimlane/ngx-datatable/themes/material.css",
              "node_modules/@swimlane/ngx-datatable/assets/icons.css",
              "node_modules/angular2-multiselect-dropdown/themes/default.theme.css",
              "src/assets/vendor/css-chart.css",
              "src/assets/scss/style.scss",
              "src/styles.scss"
            ],
            "scripts": [
              "node_modules/jquery/dist/jquery.min.js",
              "node_modules/jquery-migrate/dist/jquery-migrate.min.js",
              "node_modules/js-cookie/src/js.cookie.js",
              "node_modules/lodash/lodash.min.js",
              "node_modules/moment/min/moment.min.js",
              "node_modules/@aspnet/signalr/dist/browser/signalr.min.js",
              "node_modules/sweetalert/dist/sweetalert.min.js",
              "node_modules/block-ui/jquery.blockUI.js",
              "node_modules/spin.js/spin.min.js",
              "node_modules/spin.js/jquery.spin.js",
              "node_modules/push.js/bin/push.min.js",
              "node_modules/jquery-slimscroll/jquery.slimscroll.min.js",
              "node_modules/jquery-countto/jquery.countTo.js",
              "node_modules/flot/jquery.flot.js",
              "node_modules/flot/jquery.flot.resize.js",
              "node_modules/flot/jquery.flot.pie.js",
              "node_modules/flot/jquery.flot.categories.js",
              "node_modules/flot/jquery.flot.time.js",
              "node_modules/jquery-sparkline/jquery.sparkline.min.js",
              "node_modules/abp-web-resources/Abp/Framework/scripts/abp.js",
              "node_modules/abp-web-resources/Abp/Framework/scripts/libs/abp.jquery.js",
              "node_modules/abp-web-resources/Abp/Framework/scripts/libs/abp.blockUI.js",
              "node_modules/abp-web-resources/Abp/Framework/scripts/libs/abp.spin.js",
              "node_modules/abp-web-resources/Abp/Framework/scripts/libs/abp.sweet-alert.js",
              "node_modules/abp-web-resources/Abp/Framework/scripts/libs/abp.moment.js",
              "node_modules/toastr/toastr.js",
              "src/bsb-theme/js/admin.js",
              "src/bsb-theme/js/demo.js",
              "src/bsb-theme/js/jquery.validate.js",
              "src/assets/wheebox/RTCMultiConnection.min.js",
              "src/assets/wheebox/socket.io.js",
              "src/assets/wheebox/getHTMLMediaElement.js",
              "node_modules/constrained-editor-plugin/dist/constrainedEditorPlugin.js",
              "src/assets/monacoeEditorNonEditableSectionScript/restrictscript.js",
              "src/assets/crossWord/crossword.js"
            ],
            "vendorChunk": true,
            "extractLicenses": false,
            "buildOptimizer": false,
            "sourceMap": true,
            "optimization": false,
            "namedChunks": true
          },
          "configurations": {
            "b2cproduction": {
              "budgets": [
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.b2cprod.ts"
                }
              ]
            },
            "b2cuat": {
              "budgets": [
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.b2cuat.ts"
                }
              ]
            },
            "b2cstaging": {
              "budgets": [
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb"
                }
              ],
              "outputHashing": "all",
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.b2cstaging.ts"
                }
              ]
            },
            "production": {
              "index": {
                "input": "src/index-yaksha-prod.html",
                "output": "index.html"
              },
              "budgets": [
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb"
                }
              ],
              "outputPath": "dist/production",
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.prod.ts"
                }
              ]
            },
            "nitte": {
              "index": {
                "input": "src/index-yaksha-prod.html",
                "output": "index.html"
              },
              "budgets": [
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb"
                }
              ],
              "outputPath": "dist/nitte",
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.nitte.ts"
                }
              ]
            },
            "nseit": {
              "index": {
                "input": "src/index-yaksha-prod.html",
                "output": "index.html"
              },
              "budgets": [
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb"
                }
              ],
              "outputPath": "dist/nseit",
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.nseit.ts"
                }
              ]
            },
            "uat": {
              "index": {
                "input": "src/index-yaksha-uat.html",
                "output": "index.html"
              },
              "budgets": [
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb"
                }
              ],
              "outputPath": "dist/uat",
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.uat.ts"
                }
              ]
            },
            "staging": {
              "budgets": [
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb"
                }
              ],
              "outputPath": "dist/staging",
              "outputHashing": "all",
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.staging.ts"
                }
              ]
            },
            "hmr": {
              "budgets": [
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb"
                }
              ],
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.hmr.ts"
                }
              ]
            },
            "en": {
              "budgets": [
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb"
                }
              ],
              "i18nMissingTranslation": "error",
              "localize": [
                "en"
              ]
            }
          },
          "defaultConfiguration": ""
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "Identity:build:en"
          },
          "configurations": {
            "b2cproduction": {
              "browserTarget": "Identity:build:b2cproduction"
            },
            "production": {
              "browserTarget": "Identity:build:production"
            },
            "nitte": {
              "browserTarget": "Identity:build:nitte"
            },
            "nseit": {
              "browserTarget": "Identity:build:nseit"
            },
            "staging": {
              "browserTarget": "Identity:build:staging"
            },
            "uat": {
              "browserTarget": "Identity:build:uat"
            },
            "hmr": {
              "browserTarget": "Identity:build:hmr"
            },
            "en": {
              "browserTarget": "Identity:build:en"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "Identity:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "src/test.ts",
            "karmaConfig": "./karma.conf.js",
            "polyfills": "src/polyfills.ts",
            "scripts": [
              "node_modules/jquery/dist/jquery.min.js",
              "node_modules/jquery-migrate/dist/jquery-migrate.min.js",
              "node_modules/js-cookie/src/js.cookie.js",
              "node_modules/lodash/lodash.min.js",
              "node_modules/moment/min/moment.min.js",
              "node_modules/@aspnet/signalr/dist/browser/signalr.min.js",
              "node_modules/sweetalert/dist/sweetalert.min.js",
              "node_modules/block-ui/jquery.blockUI.js",
              "node_modules/spin.js/spin.min.js",
              "node_modules/spin.js/jquery.spin.js",
              "node_modules/push.js/bin/push.min.js",
              "node_modules/jquery-slimscroll/jquery.slimscroll.min.js",
              "node_modules/jquery-countto/jquery.countTo.js",
              "node_modules/flot/jquery.flot.js",
              "node_modules/toastr/toastr.js",
              "node_modules/flot/jquery.flot.resize.js",
              "node_modules/flot/jquery.flot.pie.js",
              "node_modules/flot/jquery.flot.categories.js",
              "node_modules/flot/jquery.flot.time.js",
              "node_modules/jquery-sparkline/jquery.sparkline.min.js",
              "node_modules/abp-web-resources/Abp/Framework/scripts/abp.js",
              "node_modules/abp-web-resources/Abp/Framework/scripts/libs/abp.jquery.js",
              "node_modules/abp-web-resources/Abp/Framework/scripts/libs/abp.blockUI.js",
              "node_modules/abp-web-resources/Abp/Framework/scripts/libs/abp.spin.js",
              "node_modules/abp-web-resources/Abp/Framework/scripts/libs/abp.sweet-alert.js",
              "node_modules/abp-web-resources/Abp/Framework/scripts/libs/abp.moment.js",
              "src/bsb-theme/js/admin.js",
              "src/bsb-theme/js/demo.js",
              "src/bsb-theme/js/jquery.validate.js",
              "node_modules/constrained-editor-plugin/dist/constrainedEditorPlugin.js",
              "src/assets/monacoeEditorNonEditableSectionScript/restrictscript.js",
              "src/assets/crossWord/crossword.js"
            ],
            "styles": [
              "node_modules/bootstrap/dist/css/bootstrap.min.css",
              "node_modules/chartist/dist/chartist.min.css",
              "node_modules/flatpickr/dist/flatpickr.css",
              "node_modules/c3/c3.min.css",
              "node_modules/datatables.net-dt/css/jquery.dataTables.css",
              "node_modules/ngx-toastr/toastr.css",
              "node_modules/quill/dist/quill.snow.css",
              "node_modules/quill/dist/quill.bubble.css",
              "node_modules/angular-calendar/css/angular-calendar.css",
              "node_modules/@swimlane/ngx-datatable/index.css",
              "node_modules/@swimlane/ngx-datatable/themes/material.css",
              "node_modules/@swimlane/ngx-datatable/assets/icons.css",
              "node_modules/angular2-multiselect-dropdown/themes/default.theme.css",
              "src/assets/vendor/css-chart.css",
              "src/assets/scss/style.scss",
              "src/styles.scss"
            ],
            "assets": [
              "src/assets",
              "src/favicon.ico",
              {
                "glob": "abp.signalr.js",
                "input": "node_modules/abp-web-resources/Abp/Framework/scripts/libs",
                "output": "/assets/abp"
              },
              {
                "glob": "abp.signalr-client.js",
                "input": "node_modules/abp-web-resources/Abp/Framework/scripts/libs",
                "output": "/assets/abp"
              },
              {
                "glob": "**/*",
                "input": "node_modules/monaco-editor",
                "output": "/assets/monaco-editor"
              }
            ]
          }
        },
        "lint": {
          "builder": "@angular-eslint/builder:lint",
          "options": {
            "lintFilePatterns": [
              "src/**/*.ts",
              "src/**/*.html"
            ]
          }
        }
      }
    },
    "Identity-e2e": {
      "root": "",
      "sourceRoot": "",
      "projectType": "application",
      "architect": {
        "e2e": {
          "builder": "@angular-devkit/build-angular:protractor",
          "options": {
            "protractorConfig": "./protractor.conf.js",
            "devServerTarget": "Identity:serve"
          }
        },
        "lint": {
          "builder": "@angular-eslint/builder:lint",
          "options": {
            "lintFilePatterns": [
              "src/**/*.ts",
              "src/**/*.html"
            ],
            "exclude": []
          }
        }
      }
    }
  },
  "defaultProject": "Identity",
  "schematics": {
    "@schematics/angular:component": {
      "prefix": "app",
      "style": "scss"
    },
    "@schematics/angular:directive": {
      "prefix": "app"
    }
  },
  "cli": {
    "analytics": false
  }
}